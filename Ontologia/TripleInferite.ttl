#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:ObjectProperty ;
                                                                              owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                                                                              rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                              rdfs:range owl:Thing ;
                                                                              rdfs:comment "A relation between collections and entities, e.g. 'my collection of saxophones includes an old Adolphe Sax original alto' (i.e. my collection has member an Adolphe Sax alto)." ;
                                                                              rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                              rdfs:label "has member"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdf:type owl:ObjectProperty ;
                                                                               rdfs:domain owl:Thing ;
                                                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> ;
                                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                               rdfs:label "is member of"@en .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#cittaHaMuseo
:cittaHaMuseo rdf:type owl:ObjectProperty ;
              owl:inverseOf :museoInCitta ;
              rdfs:domain :Citta ;
              rdfs:range :Museo ;
              rdfs:comment """Relazione tra entità della classe Città ed entità della classe Museo.
Es. Torino 'cittàHaMuseo' museo egizio"""@it ;
              rdfs:label "cittàHaMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#cittaInNazione
:cittaInNazione rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :contenutaIn ;
                rdfs:domain :Citta ;
                rdfs:range :Nazione ;
                rdfs:comment "Mostra in quale nazione è una citta"@it ;
                rdfs:label "cittaInNazione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#contenutaIn
:contenutaIn rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain :EntitàGeografica ;
             rdfs:range :EntitàGeografica ;
             rdfs:comment """Relazione tra entità della classe EntitàGeografica
Es. Torino è una città 'contenutaIn' Europa"""@it ;
             rdfs:label "contenutaIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haDescrizione
:haDescrizione rdf:type owl:ObjectProperty ;
               owl:equivalentProperty <https://w3id.org/italia/onto/l0/hasDescription> ;
               owl:inverseOf :èDescrizioneDi ,
                             <https://w3id.org/italia/onto/l0/isDescriptionOf> ;
               rdfs:domain :Opera ;
               rdfs:range :Descrizione ;
               rdfs:comment "La descrizione di un'opera"@it ;
               rdfs:label "haDescrizione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haMembroOpera
:haMembroOpera rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
               owl:inverseOf :operaMembroDi ;
               rdfs:domain :Collezione ;
               rdfs:range :Opera ;
               rdfs:comment """Relazione tra entità della classe Collezione e la classe Opera
Es. EsposizioneArteModernaMuseoD'Orsay 'haMembroOpera' IGiocatoriDiCarte"""@it ;
               rdfs:label "haMembroOpera"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzato
:haRealizzato rdf:type owl:ObjectProperty ;
              owl:inverseOf :realizzatoDa ;
              rdfs:domain :Artista ;
              rdfs:range :Opera ;
              rdfs:comment """Relazione tra entità della classe Artista ed entità della classe Opera.
Es. Giuseppe De Nittis 'haRealizzato' La colazione in Giardino"""@it ;
              rdfs:label "haRealizzato"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzatoDipinto
:haRealizzatoDipinto rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :haRealizzato ;
                     rdfs:domain :Pittore ;
                     rdfs:range :Dipinto ;
                     rdfs:comment """Relazione tra entità della classe Pittore ed entità della classe Dipinto.
Es. Leonardo Da Vinci 'haRealizzatoDipinto' la Gioconda"""@it ;
                     rdfs:label "haRealizzatoDipinto"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzatoOperaArchitettonica
:haRealizzatoOperaArchitettonica rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :haRealizzato ;
                                 rdfs:domain :Architetto ;
                                 rdfs:range :OperaArchitettonica ;
                                 rdfs:comment """Relazione tra entità della classe Architetto ed entità della classe OperaArchitettonica.
Es. Santiago Calatrava e Félix Candela 'haRealizzatoOperaArchitettonica' la Città delle Arti e delle Scienze"""@it ;
                                 rdfs:label "haRealizzatoOperaArchitettonica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzatoScultura
:haRealizzatoScultura rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :haRealizzato ;
                      rdfs:domain :Scultore ;
                      rdfs:range :Scultura ;
                      rdfs:comment """Relazione tra entità della classe Scultore ed entità della classe Scultura.
Es. Michelangelo 'haRealizzatoScultura' il David"""@it ;
                      rdfs:label "haRealizzatoScultura"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#localizzatoIn
:localizzatoIn rdf:type owl:ObjectProperty ;
               rdfs:domain :Museo ;
               rdfs:range :Nazione ;
               owl:propertyChainAxiom ( :museoInCitta
                                        :cittaInNazione
                                      ) ;
               rdfs:comment """Relazione tra entità della classe Museo ed entità della classe Nazione.
Es. il museo egizio è 'localizzatoIn' Italia"""@it ;
               rdfs:label "museoInNazione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#museoInCitta
:museoInCitta rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Museo ;
              rdfs:range :Citta ;
              rdfs:comment """Relazione tra entità della classe Museo ed entità della classe Città.
Es. il Louvre è un 'museoIncittà' Parigi"""@it ;
              rdfs:label "museoInCitta"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#operaInCitta
:operaInCitta rdf:type owl:ObjectProperty ;
              rdfs:domain :Opera ;
              rdfs:range :Citta ;
              owl:propertyChainAxiom ( :operaMembroDi
                                       :presentataIn
                                       :museoInCitta
                                     ) ;
              rdfs:comment "Identifica la posizione di un'opera nel mondo, mostrano la nazione e il continente in cui è"@it ;
              rdfs:label "operaInCitta"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#operaMembroDi
:operaMembroDi rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
               rdfs:domain :Opera ;
               rdfs:range :Collezione ;
               rdfs:comment """Relazione tra entità della classe Opera e la classe Collezione
Es. IGiocatoriDiCarte 'operaMembroDi' EsposizioneArteModernaMuseoD'Orsay"""@it ;
               rdfs:label "operaMembroDi"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#presentaIn
:presentaIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Artista ;
            rdfs:range :Museo ;
            owl:propertyChainAxiom ( :haRealizzato
                                     :operaMembroDi
                                     :presentataIn
                                   ) ;
            rdfs:comment """Relazione tra entità della classe Artista ed entità della classe Museo.
Es. Leonardo Da Vinci 'presentaIn' museo del Louvre"""@it ;
            rdfs:label "presentaIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#presentataIn
:presentataIn rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Collezione ;
              rdfs:range :Museo ;
              rdfs:comment """Relazione tra entità della classe Collezione ed entità della classe Museo.
Es. collezione di arte egiziana è 'presentataIn' museo del Louvre"""@it ;
              rdfs:label "presentataIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#realizzatoDa
:realizzatoDa rdf:type owl:ObjectProperty ;
              rdfs:domain :Opera ;
              rdfs:range :Artista ;
              rdfs:comment """Relazione tra entità della classe Opera ed entità della classe Artista.
Es. la Gioconda è stata 'realizzataDa' Leonardo Da Vinci"""@it ;
              rdfs:label "realizzatoDa"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#èDescrizioneDi
:èDescrizioneDi rdf:type owl:ObjectProperty ;
                owl:equivalentProperty <https://w3id.org/italia/onto/l0/isDescriptionOf> ;
                owl:inverseOf <https://w3id.org/italia/onto/l0/hasDescription> ;
                rdfs:domain :Descrizione ;
                rdfs:range :Opera ;
                rdfs:comment "La descrizione di un'opera"@it ;
                rdfs:label "èDescrizioneDi"@it .


###  https://w3id.org/arco/ontology/core/hasMethod
<https://w3id.org/arco/ontology/core/hasMethod> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf :haDescrizione ,
                                                                   <https://w3id.org/italia/onto/l0/hasDescription> ;
                                                owl:inverseOf <https://w3id.org/arco/ontology/core/isMethodOf> ;
                                                rdfs:domain owl:Thing ;
                                                rdfs:range <https://w3id.org/arco/ontology/core/Method> ;
                                                rdfs:comment "L'object property che collega un'entità a un metodo."@it ,
                                                             "The object property that links an entity to a method."@en ;
                                                rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
                                                rdfs:label "ha metodo"@it ,
                                                           "has method"@en ;
                                                owl:versionInfo "stabile"@it ,
                                                                "stable"@en .


###  https://w3id.org/arco/ontology/core/isMethodOf
<https://w3id.org/arco/ontology/core/isMethodOf> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf :èDescrizioneDi ,
                                                                    <https://w3id.org/italia/onto/l0/isDescriptionOf> ;
                                                 rdfs:domain <https://w3id.org/arco/ontology/core/Method> ;
                                                 rdfs:range owl:Thing ;
                                                 rdfs:comment "L'object property che collega un metodo a un'entità."@it ,
                                                              "The object property that links a method to an entity."@en ;
                                                 rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
                                                 rdfs:label "is method of"@en ,
                                                            "è metodo di"@it ;
                                                 owl:versionInfo "stabile"@it ,
                                                                 "stable"@en .


###  https://w3id.org/italia/onto/l0/hasDescription
<https://w3id.org/italia/onto/l0/hasDescription> rdf:type owl:ObjectProperty ;
                                                 owl:inverseOf <https://w3id.org/italia/onto/l0/isDescriptionOf> .


###  https://w3id.org/italia/onto/l0/isDescriptionOf
<https://w3id.org/italia/onto/l0/isDescriptionOf> rdf:type owl:ObjectProperty .


