@prefix : <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology-47#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator> .

<http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator> rdf:type owl:Ontology ;
                                                                         owl:imports <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                         <http://purl.org/dc/elements/1.1/creators> """Pier Balestrucci
Michele Lanotte"""^^xsd:string ;
                                                                         <http://purl.org/dc/elements/1.1/date> ""^^xsd:dateTime ;
                                                                         <http://purl.org/dc/elements/1.1/description> ""^^xsd:string ;
                                                                         <http://purl.org/dc/elements/1.1/title> "Museum Informator"^^xsd:string ;
                                                                         <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNamespacePrefix> ""^^xsd:string ;
                                                                         <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNampespaceUri> ""^^xsd:string ;
                                                                         rdfs:label "MuseumInformator"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creators
<http://purl.org/dc/elements/1.1/creators> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNamespacePrefix
<http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNampespaceUri
<http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNampespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#cittàHaMuseo
:cittàHaMuseo rdf:type owl:ObjectProperty ;
              owl:inverseOf :museoInCittà ;
              rdfs:domain :Città ;
              rdfs:range :Museo ;
              rdfs:comment """Relazione tra entità della classe Città ed entità della classe Museo.
Es. Torino 'cittàHaMuseo' museo egizio"""@it ;
              rdfs:label "cittàHaMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#contenutaIn
:contenutaIn rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain :EntitàGeografica ;
             rdfs:range :EntitàGeografica ;
             rdfs:comment """Relazione tra entità della classe EntitàGeografica
Es. Torino è una città 'contenutaIn' Europa"""@it ;
             rdfs:label "contenutaIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#haMembro
:haMembro rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
          rdfs:domain :Collezione ;
          rdfs:range :Opera ;
          rdfs:comment """Relazione tra entità della classe Collezione e la classe Opera
Es. EsposizioneArteModernaMuseoD'Orsay 'haMembro' IGiocatoriDiCarte"""@it ;
          rdfs:label "haMembro"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#haRealizzato
:haRealizzato rdf:type owl:ObjectProperty ;
              owl:inverseOf :realizzatoDa ;
              rdfs:domain :Artista ;
              rdfs:range :Opera ;
              rdfs:comment """Relazione tra entità della classe Artista ed entità della classe Opera.
Es. Giuseppe De Nittis 'haRealizzato' La colazione in Giardino"""@it ;
              rdfs:label "haRealizzato"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#haRealizzatoDipinto
:haRealizzatoDipinto rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :haRealizzato ;
                     rdfs:domain :Pittore ;
                     rdfs:range :Dipinto ;
                     rdfs:comment """Relazione tra entità della classe Pittore ed entità della classe Dipinto.
Es. Leonardo Da Vinci 'haRealizzatoDipinto' la Gioconda"""@it ;
                     rdfs:label "haRealizzatoDipinto"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#haRealizzatoOperaArchitettonica
:haRealizzatoOperaArchitettonica rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :haRealizzato ;
                                 rdfs:domain :Architetto ;
                                 rdfs:range :OperaArchitettonica ;
                                 rdfs:comment """Relazione tra entità della classe Architetto ed entità della classe OperaArchitettonica.
Es. Santiago Calatrava e Félix Candela 'haRealizzatoOperaArchitettonica' la Città delle Arti e delle Scienze"""@it ;
                                 rdfs:label "haRealizzatoOperaArchitettonica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#haRealizzatoScultura
:haRealizzatoScultura rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :haRealizzato ;
                      rdfs:domain :Scultore ;
                      rdfs:range :Scultura ;
                      rdfs:comment """Relazione tra entità della classe Scultore ed entità della classe Scultura.
Es. Michelangelo 'haRealizzatoScultura' il David"""@it ;
                      rdfs:label "haRealizzatoScultura"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#localizzatoIn
:localizzatoIn rdf:type owl:ObjectProperty ;
               rdfs:domain :Museo ;
               rdfs:range :Nazione ;
               owl:propertyChainAxiom ( :museoInCittà
                                        :contenutaIn
                                      ) ;
               rdfs:comment """Relazione tra entità della classe Museo ed entità della classe Nazione.
Es. il museo egizio è 'localizzatoIn' Italia"""@it ;
               rdfs:label "localizzatoIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#museoInCittà
:museoInCittà rdf:type owl:ObjectProperty ;
              rdfs:domain :Museo ;
              rdfs:range :Città ;
              rdfs:comment """Relazione tra entità della classe Museo ed entità della classe Città.
Es. il Louvre è un 'museoIncittà' Parigi"""@it ;
              rdfs:label "museoInCittà"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#presentaIn
:presentaIn rdf:type owl:ObjectProperty ;
            rdfs:domain :Artista ;
            rdfs:range :Museo ;
            owl:propertyChainAxiom ( :haRealizzato
                                     :èMembroDi
                                     :presentataIn
                                   ) ;
            rdfs:comment """Relazione tra entità della classe Artista ed entità della classe Museo.
Es. Leonardo Da Vinci 'presentaIn' museo del Louvre"""@it ;
            rdfs:label "presentaIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#presentataIn
:presentataIn rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Collezione ;
              rdfs:range :Museo ;
              rdfs:comment """Relazione tra entità della classe Collezione ed entità della classe Museo.
Es. collezione di arte egiziana è 'presentataIn' museo del Louvre"""@it ;
              rdfs:label "presentataIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#realizzatoDa
:realizzatoDa rdf:type owl:ObjectProperty ;
              rdfs:domain :Opera ;
              rdfs:range :Artista ;
              rdfs:comment """Relazione tra entità della classe Opera ed entità della classe Artista.
Es. la Gioconda è stata 'realizzataDa' Leonardo Da Vinci"""@it ;
              rdfs:label "realizzatoDa"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#èMembroDi
:èMembroDi rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
           rdfs:domain :Opera ;
           rdfs:range :Collezione ;
           rdfs:comment """Relazione tra entità della classe Opera e la classe Collezione
Es. IGiocatoriDiCarte 'èMembroDi' EsposizioneArteModernaMuseoD'Orsay"""@it ;
           rdfs:label "èMembroDi"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#nomeArtista
:nomeArtista rdf:type owl:DatatypeProperty ;
             rdfs:domain :Artista ;
             rdfs:range xsd:string ;
             rdfs:comment "Proprietà che indica il nome di un'entità della classe Artista"@it ;
             rdfs:label "nomeArtista"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#nomeCittà
:nomeCittà rdf:type owl:DatatypeProperty ;
           rdfs:domain :Città ;
           rdfs:range xsd:string ;
           rdfs:comment "Proprietà che indica il nome di un'entità della classe Città"@it ;
           rdfs:label "nomeCittà"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#nomeContinente
:nomeContinente rdf:type owl:DatatypeProperty ;
                rdfs:domain :Continente ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:oneOf [ rdf:type rdf:List ;
                                         rdf:first "Africa" ;
                                         rdf:rest [ rdf:type rdf:List ;
                                                    rdf:first "Americhe" ;
                                                    rdf:rest [ rdf:type rdf:List ;
                                                               rdf:first "Asia" ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "Europa" ;
                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                     rdf:first "Oceania" ;
                                                                                     rdf:rest rdf:nil
                                                                                   ]
                                                                        ]
                                                             ]
                                                  ]
                                       ]
                           ] ;
                rdfs:comment "Proprietà che indica il nome di un'entità della classe Continente"@it ;
                rdfs:label "nomeContinente"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#nomeMuseo
:nomeMuseo rdf:type owl:DatatypeProperty ;
           rdfs:domain :Museo ;
           rdfs:range xsd:string ;
           rdfs:comment "Proprietà che indica il nome di un'entità della classe Museo"@it ;
           rdfs:label "nomeMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#nomeNazione
:nomeNazione rdf:type owl:DatatypeProperty ;
             rdfs:domain :Nazione ;
             rdfs:range xsd:string ;
             rdfs:comment "Proprietà che indica il nome di un'entità della classe Nazione"@it ;
             rdfs:label "nomeNazione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#realizzataNel
:realizzataNel rdf:type owl:DatatypeProperty ;
               rdfs:domain :Opera ;
               rdfs:range xsd:unsignedInt ;
               rdfs:comment "Proprietà che indica l'anno di realizzazione di un'entità della classe Opera"@it ;
               rdfs:label "realizzataNel"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#titoloOpera
:titoloOpera rdf:type owl:DatatypeProperty ;
             rdfs:domain :Opera ;
             rdfs:range xsd:string ;
             rdfs:comment "Proprietà che indica il nome di un'entità della classe Opera"@it ;
             rdfs:label "titoloOpera"@it .


#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> owl:equivalentClass :Collezione .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Acquario
:Acquario rdf:type owl:Class ;
          rdfs:subClassOf :MuseoMarittimo ;
          rdfs:comment "Classe che rappresenta gli acquari"@it ;
          rdfs:label "Acquario"@it ;
          rdfs:seeAlso "http://dbpedia.org/page/Public_aquarium"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Architetto
:Architetto rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :haRealizzatoOperaArchitettonica ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :OperaArchitettonica
                                ] ;
            rdfs:subClassOf :ArtistaFigurativo ;
            rdfs:comment "Classe di artisti che realizza opere architettoniche"@it ;
            rdfs:label "Architetto"@it ;
            rdfs:seeAlso "http://dbpedia.org/ontology/Architect"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Artista
:Artista rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :haRealizzato ;
                               owl:someValuesFrom :Opera
                             ] ;
         rdfs:comment "La classe Artista rappresenta le principali categorie di artisti"@it ;
         rdfs:label "Artista"@it ;
         rdfs:seeAlso "http://dbpedia.org/ontology/Artist"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#ArtistaAudiovisivo
:ArtistaAudiovisivo rdf:type owl:Class ;
                    rdfs:subClassOf :Artista ;
                    rdfs:comment "Rappresenta la classe di artisti che realizzano opere audio-visive"@it ;
                    rdfs:label "ArtistaAudiovisivo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#ArtistaFigurativo
:ArtistaFigurativo rdf:type owl:Class ;
                   rdfs:subClassOf :ArtistaVisivo ;
                   rdfs:comment "Classe di artisti che realizza opere architettoniche, dipinti e sculture"@it ;
                   rdfs:label "ArtistaFigurativo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#ArtistaUditivo
:ArtistaUditivo rdf:type owl:Class ;
                rdfs:subClassOf :Artista ;
                rdfs:comment "Rappresenta la classe di artisti musicisti, compositori..." ;
                rdfs:label "ArtistaUditivo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#ArtistaVisivo
:ArtistaVisivo rdf:type owl:Class ;
               rdfs:subClassOf :Artista ;
               rdfs:comment "Classe di artisti che realizza opere visive"@it ;
               rdfs:label "ArtistaVisivo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CapoDiModa
:CapoDiModa rdf:type owl:Class ;
            rdfs:subClassOf :Opera ;
            rdfs:comment "Classe che rappresenta tutti i capi di moda"@it ;
            rdfs:label "CapoDiModa"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CasaMuseo
:CasaMuseo rdf:type owl:Class ;
           rdfs:subClassOf :Museo ;
           rdfs:comment "Rappresenta le case di artisti diventate musei"@it ;
           rdfs:label "CasaMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Cineasta
:Cineasta rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :haRealizzato ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Film
                              ] ;
          rdfs:subClassOf :ArtistaAudiovisivo ;
          rdfs:comment "Classe di artisti che realizza film"@it ;
          rdfs:label "Cineasta"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Città
:Città rdf:type owl:Class ;
       rdfs:subClassOf :EntitàGeografica ;
       rdfs:comment "Classe che rappresenta territori cittadini"@it ;
       rdfs:label "Città"@it ;
       rdfs:seeAlso "http://dbpedia.org/ontology/city"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CittàMoltoTuristica
:CittàMoltoTuristica rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :cittàHaMuseo ;
                                           owl:minQualifiedCardinality "4"^^xsd:nonNegativeInteger ;
                                           owl:onClass :Museo
                                         ] ;
                     rdfs:subClassOf :Città ;
                     rdfs:comment "Classe di città in cui sono presenti almeno 4 musei"@it ;
                     rdfs:label "CittàMoltoTuristica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CittàTuristica
:CittàTuristica rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :cittàHaMuseo ;
                                      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Museo
                                    ] ;
                rdfs:subClassOf :Città ;
                rdfs:comment "Classe di città in cui sono presenti almeno 4 musei"@it ;
                rdfs:label "CittàTuristica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Collezione
:Collezione rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :haMembro ;
                                  owl:someValuesFrom :Opera
                                ] ;
            rdfs:comment "Una collezione d'arte è un insieme di opere rifacente a un periodo, una cultura, un'area geografica..."@it ;
            rdfs:label "Collezione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArteAntica
:CollezioneArteAntica rdf:type owl:Class ;
                      rdfs:subClassOf :Collezione ;
                      owl:disjointWith :CollezioneArteModerna ;
                      rdfs:comment "Classe che rappresenta collezioni di opere d'arte antica"@it ;
                      rdfs:label "CollezioneArteAntica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArteDecorativa
:CollezioneArteDecorativa rdf:type owl:Class ;
                          rdfs:subClassOf :Collezione ;
                          rdfs:comment "Classe che rappresenta collezioni di opere d'arte decorativa"@it ;
                          rdfs:label "CollezioneArteDecorativa"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArteIslamica
:CollezioneArteIslamica rdf:type owl:Class ;
                        rdfs:subClassOf :Collezione ;
                        owl:disjointWith :CollezioneArteOccidentale ,
                                         :CollezioneArteOrientale ;
                        rdfs:comment "Classe che rappresenta collezioni di arte islamica"@it ;
                        rdfs:label "CollezioneArteIslamica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArteModerna
:CollezioneArteModerna rdf:type owl:Class ;
                       rdfs:subClassOf :Collezione ;
                       rdfs:comment "Classe che rappresenta collezioni di arte moderna"@it ;
                       rdfs:label "CollezioneArteModerna"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArteOccidentale
:CollezioneArteOccidentale rdf:type owl:Class ;
                           rdfs:subClassOf :Collezione ;
                           owl:disjointWith :CollezioneArteOrientale ;
                           rdfs:comment "Classe che rappresenta collezioni di opere d'arte occidentale"@it ;
                           rdfs:label "CollezioneArteOccidentale"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArteOrientale
:CollezioneArteOrientale rdf:type owl:Class ;
                         rdfs:subClassOf :Collezione ;
                         rdfs:comment "Classe che rappresenta collezioni di opere d'arte orientale"@it ;
                         rdfs:label "CollezioneArteOrientale"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneArtigianato
:CollezioneArtigianato rdf:type owl:Class ;
                       rdfs:subClassOf :Collezione ;
                       rdfs:comment "Classe che rappresenta collezioni di opere artigianali"@it ;
                       rdfs:label "CollezioneArtigianato"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneCostume
:CollezioneCostume rdf:type owl:Class ;
                   rdfs:subClassOf :Collezione ;
                   rdfs:comment "Classe che rappresenta collezioni di costumi"@it ;
                   rdfs:label "CollezioneCostume"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#CollezioneOrnamento
:CollezioneOrnamento rdf:type owl:Class ;
                     rdfs:subClassOf :Collezione ;
                     rdfs:comment "Classe che rappresenta collezioni di opere ornamentali"@it ;
                     rdfs:label "CollezioneOrnamento"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Continente
:Continente rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :Africa
                                              :Americhe
                                              :Asia
                                              :Europa
                                              :Oceania
                                            )
                                ] ;
            rdfs:subClassOf :EntitàGeografica ;
            rdfs:comment """Classe che rappresenta quattro continenti:
-Africa
-Americhe
-Asia
-Europa
-Oceania"""@it ;
            rdfs:label "Continente"@it ;
            rdfs:seeAlso "http://dbpedia.org/ontology/continent"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Dipinto
:Dipinto rdf:type owl:Class ;
         rdfs:subClassOf :Opera ;
         rdfs:comment "Classe che rappresenta i dipinti"@it ;
         rdfs:label "Dipinto"@it ;
         rdfs:seeAlso "http://dbpedia.org/ontology/Painting"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#EntitàGeografica
:EntitàGeografica rdf:type owl:Class ;
                  rdfs:comment "Rappresenta la classe geografica dei luoghi"^^xsd:string ;
                  rdfs:label "EntitàGeografica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Film
:Film rdf:type owl:Class ;
      rdfs:subClassOf :Opera ;
      rdfs:comment "Classe che rappresenta opere cinematografiche"@it ;
      rdfs:label "Film"@it ;
      rdfs:seeAlso "http://dbpedia.org/ontology/Film"^^xsd:string ,
                   "http://dbpedia.org/page/Film"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator#Fotografia
:Fotografia rdf:type owl:Class ;
            rdfs:subClassOf :Opera ;
            rdfs:comment "Classe che rappresenta opere di tipo fotografico"@it ;
            rdfs:label "Fotografia"@it ;
