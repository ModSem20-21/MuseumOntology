@prefix : <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix museum: <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#> .
@base <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#> .

<http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#> rdf:type owl:Ontology ;
                                                                        owl:imports <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                        <http://purl.org/dc/elements/1.1/creators> """Pier Balestrucci
Michele Lanotte"""^^xsd:string ;
                                                                        <http://purl.org/dc/elements/1.1/date> ""^^xsd:dateTime ;
                                                                        <http://purl.org/dc/elements/1.1/description> ""^^xsd:string ;
                                                                        <http://purl.org/dc/elements/1.1/title> "Museum Informator"^^xsd:string ;
                                                                        <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology> ;
                                                                        <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNamespacePrefix> "museum"^^xsd:string ;
                                                                        <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNampespaceUri> "http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#"^^xsd:string ;
                                                                        museum:reusesPatternAsTemplate <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl> ;
                                                                        rdfs:label "MuseumInformator"^^xsd:string .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creators
<http://purl.org/dc/elements/1.1/creators> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements
<http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#coversRequirements> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNamespacePrefix
<http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNampespaceUri
<http://www.semanticweb.org/utente/ontologies/2020/11/MuseumInformator/vann:preferredNampespaceUri> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#reusesPatternAsTemplate
museum:reusesPatternAsTemplate rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#cittaHaMuseo
museum:cittaHaMuseo rdf:type owl:ObjectProperty ;
                    owl:inverseOf museum:museoInCitta ;
                    rdfs:domain museum:Citta ;
                    rdfs:range museum:Museo ;
                    rdfs:comment """Relazione tra entità della classe Città ed entità della classe Museo.
Es. Torino 'cittàHaMuseo' museo egizio"""@it ;
                    rdfs:label "cittàHaMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#cittaInNazione
museum:cittaInNazione rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf museum:contenutaIn ;
                      rdfs:domain museum:Citta ;
                      rdfs:range museum:Nazione ;
                      rdfs:comment "Mostra in quale nazione è una citta"@it ;
                      rdfs:label "cittaInNazione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#contenutaIn
museum:contenutaIn rdf:type owl:ObjectProperty ,
                            owl:TransitiveProperty ;
                   rdfs:domain museum:EntitàGeografica ;
                   rdfs:range museum:EntitàGeografica ;
                   rdfs:comment """Relazione tra entità della classe EntitàGeografica
Es. Torino è una città 'contenutaIn' Europa"""@it ;
                   rdfs:label "contenutaIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haDescrizione
museum:haDescrizione rdf:type owl:ObjectProperty ;
                     owl:equivalentProperty <https://w3id.org/italia/onto/l0/hasDescription> ;
                     owl:inverseOf museum:èDescrizioneDi ;
                     rdfs:domain museum:Opera ;
                     rdfs:range museum:Descrizione ;
                     rdfs:comment "La descrizione di un'opera"@it ;
                     rdfs:label "haDescrizione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haMembroOpera
museum:haMembroOpera rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember> ;
                     owl:inverseOf museum:operaMembroDi ;
                     rdfs:domain museum:Collezione ;
                     rdfs:range museum:Opera ;
                     rdfs:comment """Relazione tra entità della classe Collezione e la classe Opera
Es. EsposizioneArteModernaMuseoD'Orsay 'haMembroOpera' IGiocatoriDiCarte"""@it ;
                     rdfs:label "haMembroOpera"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzato
museum:haRealizzato rdf:type owl:ObjectProperty ;
                    owl:inverseOf museum:realizzatoDa ;
                    rdfs:domain museum:Artista ;
                    rdfs:range museum:Opera ;
                    rdfs:comment """Relazione tra entità della classe Artista ed entità della classe Opera.
Es. Giuseppe De Nittis 'haRealizzato' La colazione in Giardino"""@it ;
                    rdfs:label "haRealizzato"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzatoDipinto
museum:haRealizzatoDipinto rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf museum:haRealizzato ;
                           rdfs:domain museum:Pittore ;
                           rdfs:range museum:Dipinto ;
                           rdfs:comment """Relazione tra entità della classe Pittore ed entità della classe Dipinto.
Es. Leonardo Da Vinci 'haRealizzatoDipinto' la Gioconda"""@it ;
                           rdfs:label "haRealizzatoDipinto"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzatoOperaArchitettonica
museum:haRealizzatoOperaArchitettonica rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf museum:haRealizzato ;
                                       rdfs:domain museum:Architetto ;
                                       rdfs:range museum:OperaArchitettonica ;
                                       rdfs:comment """Relazione tra entità della classe Architetto ed entità della classe OperaArchitettonica.
Es. Santiago Calatrava e Félix Candela 'haRealizzatoOperaArchitettonica' la Città delle Arti e delle Scienze"""@it ;
                                       rdfs:label "haRealizzatoOperaArchitettonica"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#haRealizzatoScultura
museum:haRealizzatoScultura rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf museum:haRealizzato ;
                            rdfs:domain museum:Scultore ;
                            rdfs:range museum:Scultura ;
                            rdfs:comment """Relazione tra entità della classe Scultore ed entità della classe Scultura.
Es. Michelangelo 'haRealizzatoScultura' il David"""@it ;
                            rdfs:label "haRealizzatoScultura"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#localizzatoIn
museum:localizzatoIn rdf:type owl:ObjectProperty ;
                     rdfs:domain museum:Museo ;
                     rdfs:range museum:Nazione ;
                     owl:propertyChainAxiom ( museum:museoInCitta
                                              museum:cittaInNazione
                                            ) ;
                     rdfs:comment """Relazione tra entità della classe Museo ed entità della classe Nazione.
Es. il museo egizio è 'localizzatoIn' Italia"""@it ;
                     rdfs:label "museoInNazione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#museoInCitta
museum:museoInCitta rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain museum:Museo ;
                    rdfs:range museum:Citta ;
                    rdfs:comment """Relazione tra entità della classe Museo ed entità della classe Città.
Es. il Louvre è un 'museoIncittà' Parigi"""@it ;
                    rdfs:label "museoInCitta"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#operaInCitta
museum:operaInCitta rdf:type owl:ObjectProperty ;
                    rdfs:domain museum:Opera ;
                    rdfs:range museum:Citta ;
                    owl:propertyChainAxiom ( museum:operaMembroDi
                                             museum:presentataIn
                                             museum:museoInCitta
                                           ) ;
                    rdfs:comment "Identifica la posizione di un'opera nel mondo, mostrano la nazione e il continente in cui è"@it ;
                    rdfs:label "operaInCitta"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#operaMembroDi
museum:operaMembroDi rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf> ;
                     rdfs:domain museum:Opera ;
                     rdfs:range museum:Collezione ;
                     rdfs:comment """Relazione tra entità della classe Opera e la classe Collezione
Es. IGiocatoriDiCarte 'operaMembroDi' EsposizioneArteModernaMuseoD'Orsay"""@it ;
                     rdfs:label "operaMembroDi"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#presentaIn
museum:presentaIn rdf:type owl:ObjectProperty ;
                  rdfs:domain museum:Artista ;
                  rdfs:range museum:Museo ;
                  owl:propertyChainAxiom ( museum:haRealizzato
                                           museum:operaMembroDi
                                           museum:presentataIn
                                         ) ;
                  rdfs:comment """Relazione tra entità della classe Artista ed entità della classe Museo.
Es. Leonardo Da Vinci 'presentaIn' museo del Louvre"""@it ;
                  rdfs:label "presentaIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#presentataIn
museum:presentataIn rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain museum:Collezione ;
                    rdfs:range museum:Museo ;
                    rdfs:comment """Relazione tra entità della classe Collezione ed entità della classe Museo.
Es. collezione di arte egiziana è 'presentataIn' museo del Louvre"""@it ;
                    rdfs:label "presentataIn"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#realizzatoDa
museum:realizzatoDa rdf:type owl:ObjectProperty ;
                    rdfs:domain museum:Opera ;
                    rdfs:range museum:Artista ;
                    rdfs:comment """Relazione tra entità della classe Opera ed entità della classe Artista.
Es. la Gioconda è stata 'realizzataDa' Leonardo Da Vinci"""@it ;
                    rdfs:label "realizzatoDa"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#èDescrizioneDi
museum:èDescrizioneDi rdf:type owl:ObjectProperty ;
                      owl:equivalentProperty <https://w3id.org/italia/onto/l0/isDescriptionOf> ;
                      rdfs:domain museum:Descrizione ;
                      rdfs:range museum:Opera ;
                      rdfs:comment "La descrizione di un'opera"@it ;
                      rdfs:label "èDescrizioneDi"@it .


###  https://w3id.org/arco/ontology/core/hasMethod
<https://w3id.org/arco/ontology/core/hasMethod> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <https://w3id.org/italia/onto/l0/hasDescription> ;
                                                owl:inverseOf <https://w3id.org/arco/ontology/core/isMethodOf> ;
                                                rdfs:domain owl:Thing ;
                                                rdfs:range <https://w3id.org/arco/ontology/core/Method> ;
                                                rdfs:comment "L'object property che collega un'entità a un metodo."@it ,
                                                             "The object property that links an entity to a method."@en ;
                                                rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
                                                rdfs:label "ha metodo"@it ,
                                                           "has method"@en ;
                                                owl:versionInfo "stabile"@it ,
                                                                "stable"@en .


###  https://w3id.org/arco/ontology/core/isMethodOf
<https://w3id.org/arco/ontology/core/isMethodOf> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf <https://w3id.org/italia/onto/l0/isDescriptionOf> ;
                                                 rdfs:domain <https://w3id.org/arco/ontology/core/Method> ;
                                                 rdfs:range owl:Thing ;
                                                 rdfs:comment "L'object property che collega un metodo a un'entità."@it ,
                                                              "The object property that links a method to an entity."@en ;
                                                 rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
                                                 rdfs:label "is method of"@en ,
                                                            "è metodo di"@it ;
                                                 owl:versionInfo "stabile"@it ,
                                                                 "stable"@en .


###  https://w3id.org/italia/onto/l0/hasDescription
<https://w3id.org/italia/onto/l0/hasDescription> rdf:type owl:ObjectProperty .


###  https://w3id.org/italia/onto/l0/isDescriptionOf
<https://w3id.org/italia/onto/l0/isDescriptionOf> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#contenutoDescrizione
museum:contenutoDescrizione rdf:type owl:DatatypeProperty ;
                            rdfs:domain museum:Descrizione ;
                            rdfs:comment "Indica il contenuto di una descrizione di un'opera"@it ;
                            rdfs:label "contenutoDescrizione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#nomeArtista
museum:nomeArtista rdf:type owl:DatatypeProperty ;
                   rdfs:domain museum:Artista ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Proprietà che indica il nome di un'entità della classe Artista"@it ;
                   rdfs:label "nomeArtista"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#nomeCittà
museum:nomeCittà rdf:type owl:DatatypeProperty ;
                 rdfs:domain museum:Citta ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Proprietà che indica il nome di un'entità della classe Città"@it ;
                 rdfs:label "nomeCittà"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#nomeContinente
museum:nomeContinente rdf:type owl:DatatypeProperty ;
                      rdfs:domain museum:Continente ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:oneOf [ rdf:type rdf:List ;
                                               rdf:first "Africa" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "Americhe" ;
                                                          rdf:rest [ rdf:type rdf:List ;
                                                                     rdf:first "Asia" ;
                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                rdf:first "Europa" ;
                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                           rdf:first "Oceania" ;
                                                                                           rdf:rest rdf:nil
                                                                                         ]
                                                                              ]
                                                                   ]
                                                        ]
                                             ]
                                 ] ;
                      rdfs:comment "Proprietà che indica il nome di un'entità della classe Continente"@it ;
                      rdfs:label "nomeContinente"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#nomeMuseo
museum:nomeMuseo rdf:type owl:DatatypeProperty ;
                 rdfs:domain museum:Museo ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Proprietà che indica il nome di un'entità della classe Museo"@it ;
                 rdfs:label "nomeMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#nomeNazione
museum:nomeNazione rdf:type owl:DatatypeProperty ;
                   rdfs:domain museum:Nazione ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Proprietà che indica il nome di un'entità della classe Nazione"@it ;
                   rdfs:label "nomeNazione"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#realizzataNel
museum:realizzataNel rdf:type owl:DatatypeProperty ;
                     rdfs:domain museum:Opera ;
                     rdfs:range xsd:unsignedInt ;
                     rdfs:comment "Proprietà che indica l'anno di realizzazione di un'entità della classe Opera"@it ;
                     rdfs:label "realizzataNel"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#titoloOpera
museum:titoloOpera rdf:type owl:DatatypeProperty ;
                   rdfs:domain museum:Opera ;
                   rdfs:range xsd:string ;
                   rdfs:comment "Proprietà che indica il nome di un'entità della classe Opera"@it ;
                   rdfs:label "titoloOpera"@it .


#################################################################
#    Classes
#################################################################

###  http://erlangen-crm.org/200717/E71_Human-Made_Thing
<http://erlangen-crm.org/200717/E71_Human-Made_Thing> rdf:type owl:Class ;
                                                      owl:equivalentClass <http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#CoseFatteDall'uomo> ;
                                                      rdfs:comment """Scope note:
This class comprises discrete, identifiable human-made items that are documented as single units.

These items are either intellectual products or human-made physical things, and are characterized by relative stability. They may for instance have a solid physical form, an electronic encoding, or they may be logical concepts or structures. 


Examples:
- Beethoven’s 5th Symphony (E73) (Lockwood, 2015)
- Michelangelo’s David (Paoletti, 2015)
- Einstein’s Theory of General Relativity (E73) (Hartle, 2003)
- the taxon ‘Fringilla coelebs Linnaeus,1758’ (E55) (Sinkevicius and Narusevicius, 2002)


In First Order Logic:
E71(x) ⊃ E70(x)"""@en ;
                                                      rdfs:isDefinedBy <http://erlangen-crm.org/200717/> ;
                                                      rdfs:label "E71 Man-Made Thing"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class ;
                                                                               owl:equivalentClass museum:Collezione .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#Acquario
museum:Acquario rdf:type owl:Class ;
                rdfs:subClassOf museum:MuseoMarittimo ;
                rdfs:comment "Classe che rappresenta gli acquari"@it ;
                rdfs:label "Acquario"@it ;
                rdfs:seeAlso "http://dbpedia.org/page/Public_aquarium"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#Architetto
museum:Architetto rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty museum:haRealizzatoOperaArchitettonica ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass museum:OperaArchitettonica
                                      ] ;
                  rdfs:subClassOf museum:ArtistaFigurativo ;
                  rdfs:comment "Classe di artisti che realizza opere architettoniche"@it ;
                  rdfs:label "Architetto"@it ;
                  rdfs:seeAlso "http://dbpedia.org/ontology/Architect"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#Artista
museum:Artista rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty museum:haRealizzato ;
                                     owl:someValuesFrom museum:Opera
                                   ] ;
               rdfs:comment "La classe Artista rappresenta le principali categorie di artisti"@it ;
               rdfs:label "Artista"@it ;
               rdfs:seeAlso "http://dbpedia.org/ontology/Artist"^^xsd:string .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#ArtistaAudiovisivo
museum:ArtistaAudiovisivo rdf:type owl:Class ;
                          rdfs:subClassOf museum:Artista ;
                          rdfs:comment "Rappresenta la classe di artisti che realizzano opere audio-visive"@it ;
                          rdfs:label "ArtistaAudiovisivo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#ArtistaFigurativo
museum:ArtistaFigurativo rdf:type owl:Class ;
                         rdfs:subClassOf museum:ArtistaVisivo ;
                         rdfs:comment "Classe di artisti che realizza opere architettoniche, dipinti e sculture"@it ;
                         rdfs:label "ArtistaFigurativo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#ArtistaUditivo
museum:ArtistaUditivo rdf:type owl:Class ;
                      rdfs:subClassOf museum:Artista ;
                      rdfs:comment "Rappresenta la classe di artisti musicisti, compositori..." ;
                      rdfs:label "ArtistaUditivo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#ArtistaVisivo
museum:ArtistaVisivo rdf:type owl:Class ;
                     rdfs:subClassOf museum:Artista ;
                     rdfs:comment "Classe di artisti che realizza opere visive"@it ;
                     rdfs:label "ArtistaVisivo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#CapoDiModa
museum:CapoDiModa rdf:type owl:Class ;
                  rdfs:subClassOf museum:Opera ;
                  rdfs:comment "Classe che rappresenta tutti i capi di moda"@it ;
                  rdfs:label "CapoDiModa"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#CasaMuseo
museum:CasaMuseo rdf:type owl:Class ;
                 rdfs:subClassOf museum:Museo ;
                 rdfs:comment "Rappresenta le case di artisti diventate musei"@it ;
                 rdfs:label "CasaMuseo"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#Cineasta
museum:Cineasta rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty museum:haRealizzato ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass museum:Film
                                    ] ;
                rdfs:subClassOf museum:ArtistaAudiovisivo ;
                rdfs:comment "Classe di artisti che realizza film"@it ;
                rdfs:label "Cineasta"@it .


###  http://www.semanticweb.org/utente/ontologies/2020/11/MuseumOntology#Citta
museum:Citta rdf:type owl:Class ;
             rdfs:subClassOf museum:EntitàGeografica ;
             rdfs:comment "Classe che rappresenta territori cittadini"@it ;
